<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="header">
        <p><strong>Sign In</strong></p>
      </div>





  <div class="field">
   <%#= f.label :email %>
    <%= f.email_field :email, {id: "username", class: "button", placeholder: "Username", autofocus: true} %><br>
  <!-- </div> -->

  <!-- <div class="field"> -->
    <%#= f.label :password %>
    <%= f.password_field :password, {id: "password", class: "button", placeholder: "Password", autocomplete: "off"} %>

  </div>

  <div class="optional">
<!--   <a href="#">Forgot password?</a> -->

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Forgot your password?", new_password_path(resource_name) %>
<% end -%><span id="space"></span>



  <% if devise_mapping.rememberable? -%>
    <!-- <div class="field"> -->
      <%= f.check_box :remember_me, {id: "remember"} %><span></span>
      <%= f.label :remember_me %>
    </div>
  <% end -%>
   </div>

  <div class="actions">
    <%= f.submit " Sign In", {id: "submit", class: "button"} %>
  </div>





      <!-- <div class="header">
        <p><strong>Sign In</strong></p>
      </div> -->
      <!-- <div>
        <input type="text" class="button" id="username" name="username" placeholder="Username"><br>
        <input type="password" class="button" id="password" name="password" placeholder="Password"><br>
        <input type="submit" class="button" id="submit" value="Sign In"><br>
      </div>
      <div class="optional">
        <a href="#">Forgot password?</a>
        <span id="space"></span>
        <input type="checkbox" id="remember" name="remember">
          <label for="remember"><span></span>Remember me</label>
      </div> -->
      <div class="footer">
        <p>New here? <span><%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name) %><br />
<% end -%></span></p>
      </div>


<% end %>

<%#= render "devise/shared/links" %>